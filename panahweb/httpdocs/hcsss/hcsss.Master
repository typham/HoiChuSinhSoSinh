                        <%="<img src=\"" + listnew[i].NewImage.ToString() + "\" align=\"left\" border=\"0\" width=\"400\" height=\"254\" />"%></a>
                    <span>
                        <%="<a href=\"/" + listnew[i].CategoryPath + "/" + listnew[i].NewId + "-" + listnew[i] .AliasUrl+ ".html\">" + listnew[i].NewName + "</a>"
                        %>
                    </span>
                </div>
                <%} %>
                <ul class="ui-tabs-nav">
                    <%for (int i = 0; i < listnew.Count; i++)
                      {%>
                    <li class="ui-tabs-nav-item" id="nav-fragment-<%=i+1 %>"><a href="#fragment-<%=i+1 %>">
                        <span>
                            <% if (listnew[i].NewName.Length > 100)
                               { %>
                            <%=listnew[i].NewName.Substring(0, 100)%>...</span></a> </li>
                    <%}
                               else
                               {%>
                    <%=listnew[i].NewName%></span></a> </li>
                    <%} %>
                    <%} %>
                </ul>
            </div>
            <div id="liveshow_right">
                <a href="#">
                    <img src="/Images/WebImage/tracuu.jpg" border="0" /></a> <a href="#">
                    <img src="/Images/WebImage/dangki.gif" border="0" /></a>
            </div>
        </div>
        <div id="body">
