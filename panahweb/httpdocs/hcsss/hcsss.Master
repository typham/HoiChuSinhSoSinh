                <div id="menu_doc">
                    <ul class="menu">
                        <%if (menuList.Count > 0)
                          {%>
                        <% for (int i = 0; i < menuList.Count; i++)
                           { %>
                        <% if (menuList[i].items != null)
                           { %>
                        <%="<li class='expand'><a href='#' onclick=\"showHide('submenu" + i + "')\">" + menuList[i].Name + "</a>" %>
                        <%="<ul class='acitem'><div id='submenu" + i + "' class='hide'>"%>
                        <%for (int j = 0; j < menuList[i].items.Count; j++)
                          {%>
                        <%="<li><a href='#'>" + menuList[i].items[j].Name + " </a></li>"%>
                        <%  } %>
                        <%="</div></ul>"%>
                        <%="</li>" %>
                        <% } %>
                        <%else
                           {%>
                        <%="<li class=''><a href='"+menuList[i].Url+"'>" + menuList[i].Name + "</a></li>"%>
                        <%  } %>
                        <% } %>
                        <% } %>
                    </ul>
                </div>
