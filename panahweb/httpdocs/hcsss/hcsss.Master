                <li style="border: 0px; margin: 0; padding: 8px 25px 0 0px; width: 200px; float: right;">
                    <div id="search">
                        <label for="search">
                        </label>
                        <input type="text" onfocus="this.value=''" onblur="if (this.value=='') this.value='Từ khóa'"
