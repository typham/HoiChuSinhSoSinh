    <script type="text/javascript">
        $(document).ready(function () {
            $("#bottom").corner("8px");
//            $('img').each(function () {
//                if ($(this).attr('src').length == 0 || $(this).attr('src').indexOf('http://') == -1) $(this).hide();
            //            });
            $('img').each(function () {
                if ($(this).attr('src').length == 0 ) $(this).hide();
            });
        });
        $(document).ready(function () {
            $("#featured > ul").tabs({
                fx: {
                    opacity: "toggle"
                }
            }).tabs("rotate", 10000, true);
        });

        var layoutName = '<%=Request["layoutName"] %>';
        var CategoryID = '<%=Request["CategoryID"] %>';
    </script>
    <script language="JavaScript" type="text/JavaScript">
<!--
        menu_status = new Array();

        function showHide(theid) {
            if (document.getElementById) {
                var switch_id = document.getElementById(theid);

                if (menu_status[theid] != 'show') {
                    switch_id.className = 'show';
                    menu_status[theid] = 'show';
                } else {
                    switch_id.className = 'hide';
                    menu_status[theid] = 'hide';
                }
            }
        }

//-->
    </script>
    <style type="text/css">
        .menu1
        {
            width: 180px;
            background-image: url(../Images/WebImage/sanpham.jpg);
            background-repeat: no-repeat;
            height: 24px;
            text-align: left;
            font-weight: bold;
            color: #FFF;
            font-size: 11px;
            text-transform: uppercase;
            color: #000;
            padding-top: 7px;
            padding-left: 25px;
        }
        
        .submenu
        {
            background-color: #CCCCCC;
            display: block;
            height: 19px;
            padding-top: 2px;
            padding-left: 37px;
            color: #333333;
            font-family: Verdana, Arial, Helvetica, sans-serif;
            font-size: 10px;
            border-top: solid 1px #000000;
        }
        
        .hide
        {
            display: none;
        }
        
        .show
        {
            display: block;
        }
    </style>
